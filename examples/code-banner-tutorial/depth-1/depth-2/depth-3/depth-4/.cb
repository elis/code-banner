explorer:
	rows:
		- sensitivity: 2
			items:
				- type: text
					text: This will appear three level up!
        - type: text
          text: "Current version: $(package.version)"
          style:
            fontSize: 24px
            color: blue
		- sensitivity: 1
			items:
        - type: svg
          url: https://cdn.worldvectorlogo.com/logos/react-2.svg
          style:
            padding: 3px
          elementStyle:
            width: 40px
				- type: text
					text: This will appear even at the parent dir level!

statusbar:
  items:
    - name: reload window button
      options:
        text: "[R] XWindow"
        visible: true
        priority: 20002
        command: "workbench.action.reloadWindow"
    - name: reload webviews button
      depth: 4
      options:
        text: "[R] XWebviews"
        visible: true
        priority: 200000
        command: "workbench.action.webview.reloadWebviewAction"

